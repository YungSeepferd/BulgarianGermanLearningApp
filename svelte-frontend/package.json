    "eslint-plugin-svelte": "^2.46.0",
    "mdsvex": "^0.12.3",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.7",
    "svelte": "^5.43.8",
    "svelte-check": "^4.3.4",
    "typescript": "~5.9.3",
